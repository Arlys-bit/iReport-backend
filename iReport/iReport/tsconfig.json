{
  "compilerOptions": {
    "baseUrl": ".",
    "strict": false,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "module": "Node16",
    "lib": [
      "ES2015",
      "DOM"
    ],
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "allowJs": true,
    "allowImportingTsExtensions": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "extends": "expo/tsconfig.base"
}
